        <div class="row more__wrapper">
            <div class="offset-xl-1 col-xl-4 d-flex flex-column col-lg-5 col-md-5 col-sm-5 col-6">
                {% if material %}
                    <span class="more__desc">Материал стен: <span>{{ material }}</span></span>
                {% endif %}
                {% if foundation %}
                    <span class="more__desc">Фундамент: <span>{{ foundation }}</span></span>
                {% endif %}
                {% if facade_decoration %}
                    <span class="more__desc">Фасад: <span>{{ facade_decoration }}</span></span>
                {% endif %}
                {% if facade_insulation %}
