            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="swiper project-slider-swiper">
                    <div class="swiper-wrapper project-slider-wrapper">
                        {% set gallery = adView.getGallery(advertisement) %}
                        {% set galleryList = gallery.items ?: [gallery.first] %}
                        {% set galleryResizeList = imageResized.resizePicture(galleryList, {'width': '1200', 'height': '610', 'mode': 'crop', 'paddings': false, 'folder': 'twig_crop_realty_view_plan'}) %}
                        {% for galleryItem in galleryResizeList %}
                            {% if galleryItem.plan %}
                                <div class="swiper-slide project-slider-slide">
                                    <picture>
                                        <img src="{{ galleryItem.src }}" alt="{{ galleryItem.description }}" class="project-slider__image">
                                    </picture>
                                </div>
                            {% endif %}
