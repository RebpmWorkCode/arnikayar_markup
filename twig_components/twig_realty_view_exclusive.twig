<section class="benefits">
    <div class="container">
        <div class="row">
            <div class="offset-xl-1 col-xl-5 d-flex flex-column col-lg-5 col-md-5 col-sm-5 col-6">
                <h2 class="benefits__sub subtitle">Преимущества</h2>

                {% if end_street == "да" or end_street == "Да" or end_street == 1 %}
                    <span class="benefits__desc grey-text">Тупиковая улица: <span>{{ end_street }}</span></span>
                {% endif %}
                {% if panoramic_windows == "да" or panoramic_windows == "Да" or panoramic_windows == 1 %}
                    <span class="benefits__desc">Панорамные окна: <span>{{ panoramic_windows }}</span></span>
                {% endif %}
                {% if aluminum_window == "да" or aluminum_window == "Да" or aluminum_window == 1 %}
                    <span class="benefits__desc grey-text">Алюминиевые оконные профили: <span>{{ aluminum_window }}</span></span>
                {% endif %}
                {% if region %}
                    <span class="benefits__desc">Участок: <span>{{ region }}</span></span>
                {% endif %}
                {% if children_playground == "да" or children_playground == "Да" or children_playground == 1 %}
                    <span class="benefits__desc grey-text">Детская площадка: <span>{{ children_playground }}</span></span>
                {% endif %}
                {% if work_out == "да" or work_out == "Да" or work_out == 1 %}
                    <span class="benefits__desc">Спортивная площадка/Зона WorkOut: <span>{{ work_out }}</span></span>
                {% endif %}
                {% if free_layout == "да" or free_layout == "Да" or free_layout == 1 %}
                    <span class="benefits__desc grey-text">Свободная планировка: <span>{{ free_layout }}</span></span>
                {% endif %}
                {% if before_school == "да" or before_school == "Да" or before_school == 1 %}
                    <span class="benefits__desc">До школы/сада пешком: <span>{{ before_school }}</span></span>
                {% endif %}
                {% if supply_exhaust == "да" or supply_exhaust == "Да" or supply_exhaust == 1 %}
                    <span class="benefits__desc grey-text">Установлена приточно-вытяжная вентиляция: <span>{{ supply_exhaust }}</span></span>
                {% endif %}
                {% if fireplace == "да" or fireplace == "Да" or fireplace == 1 %}
                    <span class="benefits__desc">Камин: <span>{{ fireplace }}</span></span>
                {% endif %}

            </div>
            <div class="parameter__col col-xl-1 justify-content-xl-start col-lg-2 d-flex justify-content-center col-md-2 col-sm-2 col-1">
                <div class="benefits__line"></div>
            </div>
            <div class="parameter__dop col-xl-4 d-flex flex-column align-items-end col-lg-5 col-md-5 col-sm-5 col-5">
                <h2 class="benefits__sub subtitle">Особенности</h2>
{#                <span class="benefits__desc-right grey-text">Парящая лестница из массива дерева</span>#}
                {% if myself_add %}
                    <span class="benefits__desc-right">{{ myself_add }}</span>
                {% endif %}
            </div>
        </div>
    </div>
</section>
{% if advertisement.Youtube.1.url %}
<section class="video">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div>
                    <iframe class="video__video" width="1100" height="587"
                            src="{{ advertisement.Youtube.1.url }}" title="YouTube video player" frameborder="0"
                            allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                            allowfullscreen></iframe>
                </div>
            </div>
        </div>
    </div>
</section>
{% endif %}
