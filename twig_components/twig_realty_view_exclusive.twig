                {% endif %}
                {% if recreation_areas %}
                    <span class="more__desc">Зоны отдыха: <span>{{ recreation_areas }}</span></span>
                {% endif %}
            </div>
            <div
                class="parameter__col offset-xl-1 col-xl-1 justify-content-xl-start col-lg-2 d-flex justify-content-center col-md-2 col-sm-2 col-1">
                <div class="more__line"></div>
            </div>
            <div class="parameter__dop col-xl-4 d-flex flex-column align-items-end col-lg-5 col-md-5 col-sm-5 col-5">
                {% if renovation %}
                    <span class="more__desc-right">Ремонт: <span>{{ renovation }}</span></span>
                {% endif %}
                {% if interior_decoration %}
                    <span class="more__desc-right">Внутренняя отделка: <span>{{ interior_decoration }}</span></span>
                {% endif %}
                {% if bath_house %}
                    <span class="more__desc-right">Баня\Сауна: <span>{{ bath_house }}</span></span>
                {% endif %}
                {% if swimming_pool %}
                    <span class="more__desc-right">Бассейн: <span>{{ swimming_pool }}</span></span>
                {% endif %}
                {% if guest_house %}
                    <span class="more__desc-right">Гостевой дом: <span>{{ guest_house }}</span></span>
                {% endif %}
                {% if pool_size %}
                    <span class="more__desc-right">Размер бассейна: <span>{{ pool_size }}</span></span>
                {% endif %}
                {% if pool_functions %}
                    <span class="more__desc-right">Функции бассейна: <span>{{ pool_functions }}</span></span>
                {% endif %}
            </div>
        </div>
    </div>
</section>
