{% if advertisement.Youtube.0.url %}
    <section class="video">
        <div class="container">
            <div class="row">
                <div class="col-12 video__col">
                    <div>
                        <iframe class="video__video" width="1100" height="587"
                                src="{{ advertisement.Youtube.0.url }}" title="YouTube video player" frameborder="0"
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                allowfullscreen></iframe>
                    </div>
                </div>
            </div>
        </div>
    </section>
